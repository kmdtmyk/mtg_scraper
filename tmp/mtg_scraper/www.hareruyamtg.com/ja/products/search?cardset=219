    <script>
    $(function () {
        setHeaderCartInfo();
    });

    function setHeaderCartInfo()
    {
        var options = {
                type: 'get',
                dataType: 'JSON',
                url: "https://www.hareruyamtg.com/ja/cart/get",
            };
        $.ajax(options)
        .done(function (data, status, jqXHR) {
            $('.cartDisplayNumber').html(data.totalQuantity);
        });
    }
</script>

